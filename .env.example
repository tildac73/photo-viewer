# Database Configuration
POSTGRES_USER=dev
POSTGRES_PASSWORD=dev
POSTGRES_DB=mydb
DATABASE_URL=postgresql://dev:dev@db:5432/mydb

# MinIO Configuration
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin
MINIO_ENDPOINT=minio:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_SECURE=false
MINIO_API_PORT=9000
MINIO_CONSOLE_PORT=9001

# MinIO Traefik Domains
MINIO_DOMAIN=minio.yourdomain.com
MINIO_CONSOLE_DOMAIN=console.minio.yourdomain.com

# Traefik Configuration
TRAEFIK_DOMAIN=traefik.yourdomain.com
TRAEFIK_USERNAME=admin
TRAEFIK_HASHED_PASSWORD=password
ACME_EMAIL=your-email@example.com

# Frontend App
FRONTEND_APP_DOMAIN=app.yourdomain.com

# Application Configuration
DEBUG=true
